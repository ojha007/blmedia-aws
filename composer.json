{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "alexusmai/laravel-file-manager": "^2.5",
        "anlutro/l4-settings": "^1.0",
        "barryvdh/laravel-elfinder": "^0.4.6",
        "cloudinary/cloudinary_php": "^1.18",
        "cviebrock/eloquent-taggable": "^7.0",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "garygreen/pretty-routes": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "jrm2k6/cloudder": "0.4.*",
        "laravel/framework": "^7.24",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.3",
        "laravelcollective/html": "^6.2",
        "laravolt/avatar": "^3.2",
        "league/flysystem-aws-s3-v3": "~1.0",
        "nwidart/laravel-modules": "^7.2",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "spatie/laravel-permission": "^3.16",
        "stevebauman/location": "^5.2",
        "yajra/laravel-auditable": "^4.3"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Http/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
